@model IEnumerable<Encuestadora_Identity2.Models.EncuestaRespondida>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.datetimeRespuestaEncuesta)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.encuesta)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.pregunta)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.opcionPregunta)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ApplicationUser)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.datetimeRespuestaEncuesta)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.encuesta.tituloEncuesta)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.pregunta.tituloPregunta)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.opcionPregunta.tituloOpcion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ApplicationUser.Id)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.EncuestaRespondidaId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.EncuestaRespondidaId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.EncuestaRespondidaId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
